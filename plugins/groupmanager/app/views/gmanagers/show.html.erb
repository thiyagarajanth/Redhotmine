<h2><%= link_to "Groups",gmanagers_path(:project_id=>params["project_id"])%>>><%= render_group_by_id(params["id"].to_i) %></h2>
<div class="tab-content" id="tab-content-general">
<div class="splitcontentleft">
<table class="group list">
<thead>
<tr>
<th class="label"><label class="user" value="user">user</label></th> 

<% for custom in render_custom_fields() %>
<th> <%= custom %> </th> 

<% end %>

</tr>
</thead>
<tbody>
<% for u in @group%>

    <tr>
        <td class="user"> <%= u["firstname"].to_s + " " + u["lastname"].to_s %> </td>
       <% dep=render_user_depart(u["id"].to_i)%>
       <% dep && dep.each do |each_dep|%>
            <td> <%= each_dep %></td>
        <% end %>
   </tr>
<% end %>
</tbody>
</table>
<%= link_to "Change", edit_gmanager_path(params["id"], :project_id=>params["project_id"]) %>
</div>
</div>
